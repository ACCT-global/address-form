{
  "name": "address-form-lib",
  "version": "1.0.0",
  "description": "address-form-lib React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component && rm -rf es/**/__mocks__ && rm -rf lib/**/__mocks__ && rm -rf es/__mocks__ && rm -rf lib/__mocks__",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --env=jsdom --watch"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "load-google-maps-api": "^1.0.0",
    "lodash": "^4.17.4",
    "msk": "^1.0.2",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-react-app": "^3.0.1",
    "enzyme": "^2.9.1",
    "eslint-config-vtex-react-app": "^1.0.4",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.5.3",
    "nwb": "^0.18.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.6.1"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testPathIgnorePatterns": ["/node_modules/", "<rootDir>/es/", "<rootDir>/lib/", "<rootDir>/umd/"],
    "modulePathIgnorePatterns": ["<rootDir>/es/", "<rootDir>/lib/", "<rootDir>/umd/"],
    "transformIgnorePatterns": [
      "[/\\\\]lib[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]umd[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]es[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)(\\?.*)?$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    }
  }
}
